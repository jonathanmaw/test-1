 	return sum;
-	
+
 }
 
 int main(int argc, char *argv[])
@@ -45,21 +45,21 @@ int main(int argc, char *argv[])
 	int alternate = 0;
 	int sum = 0;
 	int opt;
-	
+
 	while ((opt = getopt (argc, argv, "a")) != -1)
